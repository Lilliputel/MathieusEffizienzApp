<UserControl x:Class="Effizienz.Views.ViewOptionen"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel>
		<ToggleButton x:Name="ToggleButton_Theme"
					  Content="Dark!"
					  Command="{Binding CommandChangeTheme}"
					  HorizontalAlignment="Stretch" VerticalAlignment="Center"
					  Margin="0,5" />
	</StackPanel>
</UserControl>
