 <Application x:Class="EffizienzNeu.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EffizienzNeu"
			  
			 xmlns:Views="clr-namespace:EffizienzNeu.Views"
			 xmlns:ViewModels="clr-namespace:EffizienzNeu.ViewModels"
             StartupUri="windows/MainWindow.xaml">

	<Application.Resources>
		<DataTemplate DataType="{x:Type ViewModels:ViewModelKategorie}">
			<Views:ViewKategorie/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type ViewModels:ViewModelProjekt}">
			<Views:ViewProjekt/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type ViewModels:ViewModelAufgabe}">
			<Views:ViewAufgabe/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type ViewModels:ViewModelPomodoro}">
			<Views:ViewPomodoro/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type ViewModels:ViewModelÜbersicht}">
			<Views:ViewÜbersicht/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type ViewModels:ViewModelStatistik}">
			<Views:ViewStatistik/>
		</DataTemplate>


	</Application.Resources>
</Application>
