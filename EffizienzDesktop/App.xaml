<Application x:Class="EffizienzNeu.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EffizienzNeu"
			 
             StartupUri="Views/MainWindow.xaml">

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>

				<ResourceDictionary x:Name="ThemeDictionary">
					<ResourceDictionary.MergedDictionaries>
						<ResourceDictionary Source="Themes/ThemeBright.xaml"/>
					</ResourceDictionary.MergedDictionaries>
				</ResourceDictionary>

				<ResourceDictionary Source="Dictionaries/DictionaryBase.xaml"/>
				<ResourceDictionary Source="Dictionaries/DictionaryDataTemplates.xaml"/>
				
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
