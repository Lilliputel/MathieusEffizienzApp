	<DataTemplate DataType="{x:Type ViewModels:ViewModelÜbersicht}">
		<Views:ViewÜbersicht/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:ViewModelStatistik}">
		<Views:ViewStatistik/>
	</DataTemplate>
	
</ResourceDictionary>