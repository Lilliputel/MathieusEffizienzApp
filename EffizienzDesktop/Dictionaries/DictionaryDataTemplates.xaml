	<DataTemplate DataType="{x:Type ViewModels:ViewModelKategorie}">
		<Views:ViewKategorie/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:ViewModelProjekt}">
		<Views:ViewProjekt/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:ViewModelAufgabe}">
