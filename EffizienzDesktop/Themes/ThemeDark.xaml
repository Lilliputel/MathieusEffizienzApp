<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:Name="ThemeDark">

	<!--#region Vordergründe -->

	<SolidColorBrush x:Key="Foreground" Color="White"/>
	<SolidColorBrush x:Key="Fore_1" Color="WhiteSmoke"/>
	<SolidColorBrush x:Key="Fore_2" Color="DarkGray"/>
	<SolidColorBrush x:Key="Fore_3" Color="SlateGray"/>
	<!--#endregion-->


	<!--#region Hintergründe -->

	<SolidColorBrush x:Key="Background" Color="#1e2124"/>
	<SolidColorBrush x:Key="Back_1" Color="#424549"/>
	<SolidColorBrush x:Key="Back_2" Color="#36393e"/>
	<SolidColorBrush x:Key="Back_3" Color="#2a2d31"/>
	<!--#endregion-->

	
	<!--#region Highlights -->

	<SolidColorBrush x:Key="ForeHigh_1" Color="#e86af0"/>
	<SolidColorBrush x:Key="ForeHigh_2" Color="#9e379f"/>

	<SolidColorBrush x:Key="BackHigh_1" Color="#ff9aa3"/>
	<SolidColorBrush x:Key="BackHigh_2" Color="#ffb3ba"/>
	<!--#endregion-->
	
	
</ResourceDictionary>