<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:Name="ThemeLight">

	<!--#region Vordergründe -->
	
	<SolidColorBrush x:Key="Foreground" Color="Black"/>
	<SolidColorBrush x:Key="Fore_1" Color="DarkSlateGray"/>
	<SolidColorBrush x:Key="Fore_2" Color="LightSlateGray"/>
	<SolidColorBrush x:Key="Fore_3" Color="DarkGray"/>
	<!--#endregion-->

	
	<!--#region Hintergründe -->

	<SolidColorBrush x:Key="Background" Color="#f8f8fa"/>
	<SolidColorBrush x:Key="Back_1" Color="#c0c2ce"/>
	<SolidColorBrush x:Key="Back_2" Color="#d2d4dc"/>
	<SolidColorBrush x:Key="Back_3" Color="#e5e6eb"/>
	<!--#endregion-->

	
	<!--#region Highlights -->
	
	<SolidColorBrush x:Key="ForeHigh_1" Color="#00b126"/>
	<SolidColorBrush x:Key="ForeHigh_2" Color="#007e1b"/>
	
	<SolidColorBrush x:Key="BackHigh_1" Color="#aacbff"/>
	<SolidColorBrush x:Key="BackHigh_2" Color="#c4dbff"/>
	<!--#endregion-->
	
	
</ResourceDictionary>