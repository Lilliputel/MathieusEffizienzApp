	<DataTemplate DataType="{x:Type ViewModels:DashboardViewModel}">
		<Views:DashboardView/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:GanttDiagramViewModel}">
		<Views:GanttDiagramView/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:PlanViewModel}">
		<Views:PlanView/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:GoalOverviewViewModel}">
		<Views:GoalOverviewView/>
	</DataTemplate>
