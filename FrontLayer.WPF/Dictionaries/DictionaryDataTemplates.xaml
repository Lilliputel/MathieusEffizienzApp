<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					
					xmlns:Views="clr-namespace:FrontLayer.WPF.Views"
					xmlns:ViewModels="clr-namespace:LogicLayer.Views;assembly=LogicLayer">

	<!--#region ViewModels Essential  -->
	<DataTemplate DataType="{x:Type ViewModels:NewCategoryViewModel}">
		<Views:NewCategoryView/>
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:NewGoalViewModel}">
		<Views:NewGoalView/>
